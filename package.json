{
  "name": "gapseeker",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron-builder": "^19.49.4",
    "electron": "~1.7.8"
  },
  "dependencies": {
    "npm": "^5.7.1"
  },
  "build": {
        "productName": "Freeland",
        "appId": "Freeland.abc",
        "directories": {
           "output": "build"
        },
        "nsis": {
           "oneClick": false,
           "allowToChangeInstallationDirectory": true,
           "runAfterFinish": true,
           "artifactName": "${name}-${version}.${ext}",
           "createDesktopShortcut": true
        },
        "portable": {
           "artifactName": "${name}-${version}.${ext}"
        },
        "win": {
           "target": [
              "portable"
           ]
        }
     }
}
